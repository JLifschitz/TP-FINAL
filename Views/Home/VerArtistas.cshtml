 @{
    ViewData["Title"] = "Ver Artistas";
}
<!DOCTYPE html>
<html lang="en">
    <head>
        <link rel="Stylesheet" src="site.css">
        <title>VerArtistas</title> 
    </head>
    <body>
    <div class="columna">
        @foreach (Artista item in ViewBag.Artistas)
        {
            <div class ="Artistas">
                <h1>@item.nombre</h1>
                <a href='@Url.Action("DetallesArtista", item)'><img src="/img/Artistas/@item.foto"></a>
                <hi>Likes: @item.gusta</hi>
                <hi>Dislikes: @item.noGusta</hi> 
            </div>
        }
    </div>
    </body>
</html>
 @{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html lang="en">
    <head>
        <link rel="Stylesheet" src="site.css">
        <title>VerArtistas</title>
    </head>
    <body>
        @foreach (Artista item in ViewBag.Artista)
{
    <h1>Nombre:  @item.nombre</h1>
    <img src="@item.foto">
    <hi>Likes: @item.gusta</hi>
     <hi>Dislikes: @item.noGusta</hi> 
}
    </body>
